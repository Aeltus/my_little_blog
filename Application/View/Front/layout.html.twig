<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="Application/View/Front/js/jquery-2.1.1.min.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="Application/View/Front/js/bootstrap.min.js"></script>
<script src="Application/View/Front/js/wow.min.js"></script>
<script src="Application/View/Front/js/fancybox/jquery.fancybox.pack.js"></script>
<script src="Application/View/Front/js/jquery.easing.1.3.js"></script>
<script src="Application/View/Front/js/jquery.bxslider.min.js"></script>
<script src="Application/View/Front/js/functions.js"></script>
<script>wow = new WOW({}).init();</script>
<script type="text/javascript">
    $(function() {
        $(".messages").fadeOut(5000, 'linear');

        $(".next-btn").click(function(){
            var page = $("#next-page").val();
            $("#page").attr("value", page);
            $("#form-btn").click();
        });
        $(".prev-btn").click(function(){
            var page = $("#prev-page").val();
            $("#page").attr("value", page);
            $("#form-btn").click();
        });
        $(".search-btn").click(function () {
            $("#form-btn").click();
        });

        $("#note-btn").click(function (){
            $(".noteForm").css("display", "block");
            $("#note-btn").css("display", "none");
        });

        $("#addComment").click(function (){
            $(".commentForm").css("display", "block");
            $("#addComment").css("display", "none");
        });

        $("#cookies").click(function (){
            $("#cookies").css('display', 'none');
        });

    });
</script>
</body>
</html>