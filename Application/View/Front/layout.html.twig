<body>

<header>
<!--====================================================================================================================
#                                                                                                                      #
#                                                Cookies                                                               #
#                                                                                                                      #
=====================================================================================================================-->

    <div class="col-md-12 text-center" id="cookies">
        {% if messageCookies is defined %}
            <a href>
                <div class="row alert alert-danger">

                    <div class="col-md-3 col-md-offset-9 exit">
                        compris ! <i class="fa fa-times-circle-o fa-lg"></i>
                    </div>
                    <div class="col-md-12">
                        {{ messageCookies }}
                    </div>

                </div>
            </a>
        {% endif %}
    </div>

<!--=================================================================================================================-->
<!--====================================================================================================================
#                                                                                                                      #
#                                                Messages                                                              #
#                                                                                                                      #
=====================================================================================================================-->
    <div class="messages text-center col-md-12">
        <div class="row">

            {% if messagesSuccess is defined and messagesAlert is not defined %}

                <div class="alert alert-success">

                    {% for message in messagesSuccess %}
                        {{ message }} <br />
                    {% endfor %}

                </div>

            {% endif %}

            {% if messagesInfo is defined %}

                <div class="alert alert-info">

                    {% for message in messagesInfo %}
                        {{ message }} <br />
                    {% endfor %}

                </div>

            {% endif %}

            {% if messagesWarning is defined %}

                <div class="alert alert-warning">

                    {% for message in messagesWarning %}
                        {{ message }} <br />
                    {% endfor %}

                </div>

            {% endif %}

            {% if messagesDanger is defined %}

                <div class="alert alert-danger">

                    {% for message in messagesDanger %}
                        {{ message }} <br />
                    {% endfor %}

                </div>

            {% endif %}

        </div>
    </div>
<!--=================================================================================================================-->


    <nav div class="navbar navbar-default navbar-static-top" role="navigation">
        <div class="container">

            <ul class="social-network">
                <li><a href="https://twitter.com/Aeltus" target="_blank" data-placement="top" title="Twitter"><i class="fa fa-twitter fa-1x"></i></a></li>
