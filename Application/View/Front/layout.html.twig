    <title>{% block title %}{% endblock %}</title>

    <!-- Bootstrap -->
    <link href="Application/View/Front/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="Application/View/Front/css/font-awesome.min.css">
